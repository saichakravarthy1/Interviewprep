    "def two_sum(array,target):\n",
    "    n = len(array)\n",
    "    if n < 2:\n",
    "        return [0,0]\n",
    "    dic = {}\n",
    "    for i in range(n):\n",
    "        if array[i] in dic:\n",
    "            return dic[array[i]], i\n",
    "        else:\n",
    "            dic[target-array[i]] = i\n",
    "    \n",
    "\n",
    "two_sum([2,7,11,15],9)"
   ]

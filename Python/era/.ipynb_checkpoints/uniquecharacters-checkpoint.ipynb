    "def uni_char(s):\n",
    "    l = len(s)\n",
    "    if l == 0:\n",
    "        return False\n",
    "    if l == 1:\n",
    "        return True\n",
    "    for i in range(l):\n",
    "        if s[i] == s[i-1]:\n",
    "            return False\n",
    "    return True\n",
    "        "
   ]
